</head>
<body>
<header></header>
<p></p>

<div class="container">

<input ng-model="test"></input>
{{test}}

<div class="container" ng-controller="tableManage">
    <h1>Movie list</h1>
    <table class="table">
        <thead>
        <tr>
            <th>Name</th>
            <th>Provider</th>
            <th>Link</th>
        </tr>
        </thead>
        <tbody>
            <tr ng-repeat="movie in list">
                <td>{{movie.name}}</td>
                <td>{{movie.provider}}</td>
                <td>{{movie.link}}</td>
            </tr>
        </tbody>
    </table>
</div>

</div>

